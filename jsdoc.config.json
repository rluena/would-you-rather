{
  "source": {
    "include": ["src/utils", "src/selectors", "src/services"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|build)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/"
  }
}
